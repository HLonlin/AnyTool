/* components/header/header.wxss */
.page_head{
  position: fixed;
  top: 0px;
  z-index: 99;
  width: 100%;
  height: 40px;
  background-color: #F6F6F6;
}
.page_head_container{
  width: 90%;
  height: 100%;
  overflow: hidden;
  margin: 0px auto;
}
.page_head_title{
  display: inline-block;
  font-size: 12px;
  text-align: center;
  line-height: 40px;
  overflow: hidden;
  position: relative;
}
.page_head_title::before,.page_head_title::after{
  position: absolute;
  left: 50%;
  top: 10px;
  transform: translateX(-50%);
  width: 90%;
  height: 2px;
  background: #000;
  content: '';
}
.page_head_title::after{
  top: 30px;
}
.page_head_sign{
  float: right;
  font-size: 12px;
  color:#6f6f6f;
  line-height: 40px;
}